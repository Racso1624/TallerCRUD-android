        android:supportsRtl="true"
        android:theme="@style/Theme.TallerActividad">
        <activity android:name=".Suma"></activity>
        <activity android:name=".Resta"/>
        <activity android:name=".Multiplicacion"/>
        <activity android:name=".Division"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>